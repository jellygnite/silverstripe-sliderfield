{
    "name": "jellygnite/silverstripe-sliderfield",
    "description": "Simple slider field for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "slider",
        "jqueryui"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian.mooyman@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jellygnite/silverstripe-sliderfield/issues"
    },
    "require": {
        "silverstripe/vendor-plugin": "^2.0",
        "silverstripe/cms": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Jellygnite\\Sliderfield\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    }
}
